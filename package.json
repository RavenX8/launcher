{
  "name": "osirose-patcher",
  "version": "0.1.2",
  "description": "ROSE online patcher made for osirose-new",
  "main": "main.js",
  "scripts": {
    "start": "electron . --dev",
    "prod": "electron .",
    "pack": "electron-builder --dir .",
    "dist": "electron-builder"
  },
  "author": "RavenX8",
  "license": "Apache 2.0",
  "devDependencies": {
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1"
  },
  "dependencies": {
    "decompress-zip": "^0.2.2",
    "electron-dl": "^1.14.0",
    "electron-store": "^4.0.0",
    "electron-updater": "^4.1.2",
    "js-sha256": "^0.9.0"
  },
  "build": {
    "win": {
      "publish": {
        "provider": "generic",
        "url": "https://azgstudio.com/downloads",
        "channel": "latest"
      }
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "appId": "com.github.dev-osrose.patcher",
    "productName": "OsIROSE Test Server",
    "copyright": "Copyright Â© 2019 ${author}"
  }
}
